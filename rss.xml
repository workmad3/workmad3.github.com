---
layout: none
permalink: /rss
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>workmad3.com blog</title>
    <link>http://www.workmad3.com/blog.html</link>
    <description>Feed for workmad3.com blog</description>
    <lastBuildDate>{{site.time}}</lastBuildDate>
    {% for post in site.categories.blog %}
			<item>
				<title>{{ post.title }}</title>
				<description>{{ post.content | xml_escape }}</description>
				<link>{{site.url}}{{ post.url }}</link>
			</item>
		{% endfor %}
  </channel>
  <channel>
    <title>workmad3.com news</title>
    <link>http://www.workmad3.com/news.html</link>
    <description>Feed for workmad3.com news</description>
    <lastBuildDate>{{site.time}}</lastBuildDate>
    {% for post in site.categories.news %}
      <item>
        <title>{{ post.title }}</title>
        <description>{{ post.content | xml_escape }}</description>
        <link>{{site.url}}{{ post.url }}</link>
      </item>
    {% endfor %}
  </channel>
  <channel>
    <title>workmad3.com project</title>
    <link>http://www.workmad3.com/projects.html</link>
    <description>Feed for workmad3.com projects</description>
    <lastBuildDate>{{site.time}}</lastBuildDate>
    {% for post in site.categories.projects %}
      <item>
        <title>{{ post.title }}</title>
        <description>{{ post.content | xml_escape }}</description>
        <link>{{site.url}}{{ post.url }}</link>
      </item>
    {% endfor %}
  </channel>
</rss>
